<section id="<%= section_id %>%" class="section-padding">
  <div class="container text-center">
    <h1><%= title %></h1>
    <p class="lead-lg max-width-700 mb-60"><%= subtitle %></p>
  </div>

  <div class="container">
    <div class="rows portfolio-grid gallery-popup">

      <% data[data_file].each do |item| %>
        <div class="portfolio-item photography creative">
          <div class="portfolio-box">
            <% link_to image_path("#{images_dir}/#{item.image}"), class: 'porfolio-popup gallery-popup-link', title: "#{item.title} - #{item.subtitle}" do %>
              <div class="portfolio-image-wrap">
                <%= image_tag "#{images_dir}/placeholder.png" %>
              </div>
              <div class="portfolio-caption-mask">
                <div class="portfolio-caption-text">
                  <div class="portfolio-caption-tb-cell">
                    <h5 class="alt-title"><%= item.title %></h5>
                    <p><%= item.subtitle %></p>
                  </div>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      <% end %>

    </div>
  </div>

  <div class="container mt-60">
    <div class="row">
      <div class="col-sm-8">
        <h3 class="action-box-title">Like our Creative Projects?.</h3>
        <%#<p class="action-box-content">classical Latin literature from 45 BC, making it over 2000 years old Richard McClintock The generated.</p>%>
      </div>
      <div class="col-sm-4 text-right">
        <a class="scroll-down btn btn-xlg btn-color-outline" href="https://calendly.com/galas-at-home/exploration">Free Exploration Call</a>
      </div>
    </div>
  </div>

</section>